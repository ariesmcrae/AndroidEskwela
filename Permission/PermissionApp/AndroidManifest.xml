<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ariesmcrae.eskwela.permission"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

	<uses-permission android:name="com.android.browser.permission.READ_HISTORY_BOOKMARKS" />    
    <uses-permission android:name="com.ariesmcrae.eskwela.permission.DANGEROUS_ACTIVITY_PERM" />
	
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ariesmcrae.eskwela.permission.ActivityLoaderActivity"
            android:label="@string/title_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ariesmcrae.eskwela.permission.BookmarksActivity"
            android:label="@string/title_permissions" >
        </activity>
        <activity
            android:name="com.ariesmcrae.eskwela.permission.GoToDangerousActivity"
            android:label="@string/title_activity_customization" >
        </activity>
    </application>

</manifest>